    <!-- TODO -->
    <GeneDef>
        <defName>BGM_BlindingSpores</defName>
        <label>blinding spores</label>
        <description>Carriers of this gene have the ability to produce a thick cloud of smoke-like fungal spores. The cloud decreases the vision of those within and prevents turrets from targetting those within.</description>
        <iconPath>UI/Ability-Genes/Blinding_Spores</iconPath>
        <displayCategory>Ability</displayCategory>
        <abilities>
            <li>BGM_BlindingSpores</li>
        </abilities>
        <descriptionHyperlinks>
            <AbilityDef>BGM_BlindingSpores</AbilityDef>
        </descriptionHyperlinks>
        <biostatMet>-3</biostatMet>
        <biostatCpx>1</biostatCpx>
        <symbolPack>
            <wholeNameSymbols>
                <li>
                    <symbol>warcaller</symbol>
                </li>
            </wholeNameSymbols>
            <prefixSymbols>
                <li>
                    <symbol>war</symbol>
                </li>
                <li>
                    <symbol>animal</symbol>
                </li>
                <li>
                    <symbol>cry</symbol>
                </li>
            </prefixSymbols>
            <suffixSymbols>
                <li>
                    <symbol>caller</symbol>
                </li>
                <li>
                    <symbol>crier</symbol>
                </li>
                <li>
                    <symbol>hailer</symbol>
                </li>
            </suffixSymbols>
        </symbolPack>
    </GeneDef>

